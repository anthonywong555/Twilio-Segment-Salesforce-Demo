<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Access_Token_is_Not_Blank</fullName>
    <active>true</active>
    <errorConditionFormula>ISBLANK(Access_Token__c)</errorConditionFormula>
    <errorDisplayField>Access_Token__c</errorDisplayField>
    <errorMessage>Access Token can not be blank.</errorMessage>
</ValidationRule>
